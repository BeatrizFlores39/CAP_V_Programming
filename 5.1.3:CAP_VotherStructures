5.1.3:CAP_VotherStructures
#include<bits/stdc++.h>
using namespace std;

int main() {
    
    deque<int> d; 
    d.push_back(5); // [5] 
    d.push_back(2); // [5, 2] 
    d.push_front(3); // [3, 5, 2] 
    d.pop_back(); // [3, 5] 
    d.pop_front(); // [5] 
    cout << "Deque: " << d.front() << "\n";
    // Stack (Pila)
    stack<int> s; 
    s.push(2); // [2] 
    s.push(5); // [2, 5] 
    cout << "Stack top: " << s.top() << "\n"; //5
    cout << "Stack top after pop: " << s.top() << "\n"; 

    // Queue (Cola)
    queue<int> q; 
    q.push(2); // [2] 
    q.push(5); // [2, 5] 
    cout << "Queue front: " << q.front() << "\n"; 
    q.pop(); // [5] 
    cout << "Queue back: " << q.back() << "\n"; 

    return 0;
}
