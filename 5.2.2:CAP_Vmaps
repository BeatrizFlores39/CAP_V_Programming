//5.2.2:CAP_Vmaps
#include<bits/stdc++.h>
using namespace std;

int main() {
    
    map<string, int> m;
    m["monkey"] = 4;
    m["banana"] = 3;
    m["harpsichord"] = 9;

    
    cout << m["banana"] << "\n"; // 3

   
    map<string, int> m2;
    cout << m2["aybabtu"] << "\n"; // 0

    
    if (m.count("aybabtu")) {
        cout << "La clave 'aybabtu' existe en el mapa.\n";
    } else {
        cout << "La clave 'aybabtu' no existe en el mapa.\n";
    }

    
    for (auto x : m) {
        cout << x.first << " " << x.second << "\n"; // Muestra la clave y el valor
    }

    return 0;
}
